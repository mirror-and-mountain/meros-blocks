{
    "version": 3,
    "$schema": "https://schemas.wp.org/wp/6.7/theme.json",
    "settings": {
        "appearanceTools": true,
        "layout": {
            "contentSize": "1200px",
            "wideSize": "1200px"
        },
        "useRootPaddingAwareAlignments": true,
        "typography": {
            "fontFamilies": [
                {
                    "name": "Inter",
                    "slug": "inter",
                    "fontFamily": "Inter, sans-serif",
                    "fontFace": [
                        {
                            "src": [
                                "file:./assets/fonts/inter/inter-200-normal.woff2"
                            ],
                            "fontWeight": "200",
                            "fontStyle": "normal",
                            "fontFamily": "Inter"
                        },
                        {
                            "src": [
                                "file:./assets/fonts/inter/inter-500-normal.woff2"
                            ],
                            "fontWeight": "500",
                            "fontStyle": "normal",
                            "fontFamily": "Inter"
                        },
                        {
                            "src": [
                                "file:./assets/fonts/inter/inter-400-normal.woff2"
                            ],
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "fontFamily": "Inter"
                        },
                        {
                            "src": [
                                "file:./assets/fonts/inter/inter-300-normal.woff2"
                            ],
                            "fontWeight": "300",
                            "fontStyle": "normal",
                            "fontFamily": "Inter"
                        },
                        {
                            "src": [
                                "file:./assets/fonts/inter/inter-700-normal.woff2"
                            ],
                            "fontWeight": "700",
                            "fontStyle": "normal",
                            "fontFamily": "Inter"
                        },
                        {
                            "src": [
                                "file:./assets/fonts/inter/inter-600-normal.woff2"
                            ],
                            "fontWeight": "600",
                            "fontStyle": "normal",
                            "fontFamily": "Inter"
                        }
                    ]
                }
            ]
        }
    },
    "styles": {
         "typography": {
            "fontFamily": "var(--wp--preset--font-family--inter)"
        },
        "spacing": {
            "blockGap": "1.5rem",
            "padding": {
                "bottom": "0",
                "left": "20px",
                "right": "20px",
                "top": "0"
            }
        }
    },
    "templateParts": [
        {
            "area": "header",
            "name": "header"
        },
        {
            "area": "footer",
            "name": "footer"
        }
    ]
}