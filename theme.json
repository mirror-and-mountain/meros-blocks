{
    "version": 3,
    "$schema": "https://schemas.wp.org/wp/6.7/theme.json",
    "settings": {
        "appearanceTools": true,
        "layout": {
            "contentSize": "1200px",
            "wideSize": "1200px"
        },
        "useRootPaddingAwareAlignments": true
    },
    "styles": {
        "spacing": {
            "blockGap": "1.5rem",
            "padding": {
                "bottom": "0",
                "left": "20px",
                "right": "20px",
                "top": "0"
            }
        }
    },
    "templateParts": [
        {
            "area": "header",
            "name": "header"
        },
        {
            "area": "footer",
            "name": "footer"
        }
    ]
}